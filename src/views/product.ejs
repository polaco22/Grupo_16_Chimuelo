<!DOCTYPE html>
<html lang="en">
 <!-- Head -->
 <%- include('./includes/head') %> 

<body>
    <!-- Header -->
    <%- include('./includes/header') %> 

 
    <main>
        <section class="varitems">
            <h6>Productos</h6>

            <% list.forEach(products =>  { %>
                
            <article class="levelgod">
                <a href="/product/detail/<%= products.id %>"> <img class="vista" src="<%=products.image%>" alt="fotoDeAnteojo.jpg"></a>
                <h3><%=products.name%></h3>
                <h2>PRECIO $<%=products.price%></h2>
                <h2>STOCK<%=products.stock%></h2>
            </article>
			<% } ) %>

        </section>
    </main>
   <!-- Footer -->
   <%- include('./includes/footer')%> 
</body>

</html>